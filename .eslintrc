{
  "extends": ["airbnb", "airbnb/hooks"],
  "rules": {
      "no-console": "off",
      "react/jsx-filename-extension": [
          1,
          {
              "extensions": [
                  ".js",
                  ".jsx"
              ]
          }
      ],
      "react/function-component-definition": [
        0
      ],
      "react/forbid-prop-types": 0,
      "react/prop-types": 0,
      "react/jsx-no-bind": 0,
      "import/no-unresolved": [
          2,
          {
              "caseSensitive": false
          }
      ],
      "no-undef": 0
  }
}
